@Slf4j
@Component
public class SchedulingTest {

	private static final SimpleDateFormat dateFormat = new SimpleDateFormat("HH:mm:ss");
	
	@Scheduled(fixedRate = 5000 )
	private void shcedlingTask() {
		Log("");
	}
}
